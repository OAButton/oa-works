{% assign delay_count = 0 %}
{% for item in history %}
{% assign delay_count = delay_count | plus: 100 %}
<article data-sal="slide-up"  data-sal-delay="{{ delay_count }}">
  <h2 class="type-1 font-sans weight-800">{{ item.year }}</h2>

  <ul>
    {% for event in item.content %}
      <li>{{ event }}</li>
    {% endfor %}
  </ul>
</article>
{% endfor %}
